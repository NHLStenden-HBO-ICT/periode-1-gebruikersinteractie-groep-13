<Window x:Class="BeatEmApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BeatEmApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="0,225,0,-6">
        <TextBlock HorizontalAlignment="Left" Height="108" Margin="455,41,0,0" Background="#586C70" VerticalAlignment="Top" Width="248"/>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" FontFamily="Regular 400" BorderThickness="0" Background="Transparent" Margin="525,53,97,66"/>
        <Button Name="start" Background="#F3C9A6"  Content="Start" Click="OnClick1" HorizontalAlignment="Left" Margin="73,10,0,0" VerticalAlignment="Top" Height="41" Width="151"/>
        <Button Name="Leader" Background="#F3C9A6" Content="leaderboard" Click="OnClick2" HorizontalAlignment="Left" Margin="73,67,0,0" VerticalAlignment="Top" Height="41" Width="151"/>
        <Button Name="help" Background="#F3C9A6"  Content="help" Click="OnClick3" HorizontalAlignment="Left" Margin="73,123,0,0" VerticalAlignment="Top" Height="41" Width="151"/>
        <Image  HorizontalAlignment="Center" Height="211" Margin="0,-226,0,0" VerticalAlignment="Top" Width="800"/>
    </Grid>
</Window>
