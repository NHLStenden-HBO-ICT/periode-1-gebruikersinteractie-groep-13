<Window x:Class="BeatEmApp.Menu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BeatEmApp"
        mc:Ignorable="d"
        Title="Menu" Height="450" Width="800">
    <Window.Background>
        <SolidColorBrush Color="White" Opacity="0.5"></SolidColorBrush>
    </Window.Background>
    <Grid>
        <Button Name="continue" Background="#F3C9A6"  Content="Start game" Click="OnClick1" HorizontalAlignment="Center" Margin="0,102,0,0" VerticalAlignment="Top" Height="41" Width="151"/>
        <Button Name="leaderboard" Background="#F3C9A6"  Content="leaderboard" Click="OnClick2" HorizontalAlignment="Center" Margin="0,172,0,0" VerticalAlignment="Top" Height="41" Width="152"/>
        <Button Name="quit" Background="#F3C9A6"  Content="save en quit" Click="OnClick3" HorizontalAlignment="Center" Margin="0,242,0,0" VerticalAlignment="Top" Height="41" Width="152"/>
    </Grid>
</Window>
