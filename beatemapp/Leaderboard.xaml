<Window x:Class="BeatEmApp.Leaderboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BeatEmApp"
        mc:Ignorable="d"
        Title="Leaderboard" Height="450" Width="800">
    <Grid>
        <Button Name="Back" Background="#F3C9A6" Content="terug naar titel" Click="OnClick1" HorizontalAlignment="Left" VerticalAlignment="Top" Height="42" Width="125"/>
        <TextBlock HorizontalAlignment="Left" Height="264" Margin="286,101,0,0" Background="#586C70" VerticalAlignment="Top" Width="214"/>
        <ItemsControl Name="datalist" d:ItemsSource="{d:SampleData ItemCount=10}" FontFamily="Regular 400" BorderThickness="0" Background="Transparent" Margin="344,139,278,51"/>
        <TextBlock HorizontalAlignment="Left" Margin="220,21,0,0" TextWrapping="Wrap" FontWeight="Bold" Text="kom aan de top van de leaderboard en maak kans op 1 maand gratis sporten bij sportschool Kees." Background="#33D6F5"  TextAlignment="Center" VerticalAlignment="Top" Height="53" Width="346"/>
    </Grid>
</Window>
